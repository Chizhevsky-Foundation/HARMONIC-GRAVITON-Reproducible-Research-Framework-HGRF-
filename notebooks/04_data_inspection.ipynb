# Detectar TTrees u objetos Events
tree_names = []
for k in f.keys():
    try:
        obj = f[k]
        if hasattr(obj, "classname") and "TTree" in obj.classname.decode() if isinstance(obj.classname, bytes) else "TTree" in obj.classname:
            tree_names.append(k)
    except Exception:
        pass

# Fallback: muchos ROOTs usan "Events" como nombre
if not tree_names:
    if "Events" in f.keys():
        tree_names = ["Events"]

print("Detected TTrees:", tree_names)
if not tree_names:
    raise SystemExit("No TTrees found for inspección automática.")
tree_name = tree_names[0]
print("Using tree:", tree_name)
tree = f[tree_name]
print("Number of entries (approx):", tree.num_entries)
